<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minWidth="-Infinity" prefHeight="400.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.GuiControllers.WebsitesEditWindow">
   <children>
      <Label alignment="CENTER" layoutX="-1.0" layoutY="1.0" prefHeight="51.0" prefWidth="380.0" style="-fx-background-color: #555555;" text="      Edit the list of websites to block">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <ListView fx:id="websitesToBlockListView" layoutX="-1.0" layoutY="52.0" prefHeight="318.0" prefWidth="380.0" />
      <Button fx:id="AddNewWebsiteButton" layoutX="312.0" layoutY="368.0" mnemonicParsing="false" onAction="#addNewWebsite" prefHeight="30.0" prefWidth="66.0" text="Add " />
      <TextField fx:id="websiteToBlockField" layoutX="-1.0" layoutY="368.0" onAction="#addNewWebsite" prefHeight="30.0" prefWidth="314.0" />
      <Button fx:id="DeleteWebsiteButton" layoutX="312.0" layoutY="337.0" mnemonicParsing="false" onAction="#deleteWebsite" prefHeight="30.0" prefWidth="66.0" text="Delete" />
   </children>
</AnchorPane>
